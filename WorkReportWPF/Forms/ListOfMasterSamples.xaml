<Page x:Class="WorkReportWPF.ListOfMasterSamples"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WorkReportWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ListOfMasterSamples">

    <Grid>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,415" Foreground="white">

        </ListBox>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=10}" Margin="10,35,10,10" Background="Transparent" Foreground="White"/>
    </Grid>
</Page>
